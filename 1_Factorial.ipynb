        "def factorial(n):\n",
        "  return 1 if(n==1 or n==0) else n*factorial(n-1);\n",
        "\n",
        "#driver code\n",
        "\n",
        "num = int(input(\"number: \")) \n",
        "print(\"Factorial of {0} is {1}\".format(num,factorial(num)))"
